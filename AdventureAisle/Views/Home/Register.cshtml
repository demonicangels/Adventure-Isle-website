@{
    @model AdventureAisle.Views.Home.RegisterModel

}
<link href="~/css/login.css" rel="stylesheet" type="text/css"/>
<form method="post">
 <div class="container">
    <label ><b>Username</b></label>
    <input placeholder="Enter Username" name="username" required>

    <label ><b>Password</b></label>
        <input class="pass" type="password" placeholder="Enter Password" name="psw" required>

        <button asp-controller="Home" asp-action="LoginFormValidate" type="submit" value="Login" style="margin-left: 940px;">Register</button>
</div>
</form>